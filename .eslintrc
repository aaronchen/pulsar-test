{
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "extends": [
    "prettier-standard",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "prettier/prettier": "off"
  }
}
